{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Application",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/bin/codeforces_client",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
